generator client {
  provider = "prisma-client-ts"
  output   = "../src/prisma/generated"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Class {
  id   Int    @id @default(autoincrement())
  name String
}

model Week {
  id   Int    @id @default(autoincrement())
  value String @unique
  label String
}